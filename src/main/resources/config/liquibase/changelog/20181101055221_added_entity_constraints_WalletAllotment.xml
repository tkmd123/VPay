<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity WalletAllotment.
    -->
    <changeSet id="20181101055221-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="partner_id"
                                 baseTableName="wallet_allotment"
                                 constraintName="fk_wallet_allotment_partner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="partner"/>

        <addForeignKeyConstraint baseColumnNames="wallet_id"
                                 baseTableName="wallet_allotment"
                                 constraintName="fk_wallet_allotment_wallet_id"
                                 referencedColumnNames="id"
                                 referencedTableName="wallet"/>

    </changeSet>
</databaseChangeLog>
